<core:View controllerName="de.kjumybit.fhem.controller.Navigation"
	xmlns:html="http://www.w3.org/1999/xhtml" 
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m" 
	xmlns:s="sap.m.semantic"
	xmlns:t="sap.tnt"
	displayBlock="true">
	<s:MasterPage id="viewNavigation" 
						title="{i18n>navTitle}"
						showNavButton="true" 
						navButtonPress="onPressNavigationBack">
		<s:content>
			<t:SideNavigation id="sideNavigation" width="320px" />
		</s:content>

		<s:customFooterContent>
			<OverflowToolbarButton icon="sap-icon://action-settings" />
		</s:customFooterContent>
		
	</s:MasterPage>
</core:View>