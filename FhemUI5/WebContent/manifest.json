{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "de.kjumybit.fhem",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}"
	},

	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},

	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "de.kjumybit.fhem.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": false,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "de.kjumybit.fhem.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
		  	"config": {
		  		"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "de.kjumybit.fhem.view",
				"controlId": "app"
	 	 	},
	  		"routes": [{
		  		"pattern": "",
		  		"viewName": "App",
		  		"name": "App",
		  		"target": ["detail", "master"]		  		  
			}],
			"targets": {
				"master": {
					"viewName": "Master",
					"viewLevel": 1,
					"controlAggregation": "masterPages"				
				},
				"detail": {
					"viewName": "Detail",
					"viewLevel": 2,
					"controlAggregation": "detailPages"				
				}
			}			
		}
  	}	
}