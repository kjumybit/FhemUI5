<core:View controllerName="de.kjumybit.fhem.controller.Navigation"
	xmlns:html="http://www.w3.org/1999/xhtml" 
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns:m="sap.m" 
	xmlns:semantic="sap.m.semantic"
	xmlns:t="sap.tnt"
	displayBlock="true">
	<semantic:MasterPage id="viewMaster" 
						title="{i18n>masterTitle}"
						titleLevel="Auto"
						showNavButton="true" 
						showSubHeader="false" 
						showFooter="true"
						navButtonPress="onPressMasterBack" 
						xmlns="sap.m.semantic">
		<!-- ><subHeader></subHeader> --> <!-- sap.m.IBar -->
		<content>
			<t:SideNavigation id="sideNavigation" itemSelect="onNavItemSelect" width="100%">
            </t:SideNavigation>
		</content> <!-- sap.ui.core.Control -->
		<filter></filter> <!-- sap.m.semantic.IFilter -->
	</semantic:MasterPage>
</core:View>